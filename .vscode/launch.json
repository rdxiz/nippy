    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Django",
            "type": "debugpy",
            "request": "launch",
            "args": [
                "runserver"
            ],
            "django": true,
            "autoStartBrowser": false,
            "program": "${workspaceFolder}\\src\\manage.py",
            "env": {
                "FFMPEG_PATH": "ffmpeg",
                "FFPROBE_PATH": "ffprobe",
                "DEFAULT_DB_PWD": "755080f72c164f56b0038b2ebaacb8c2"
            }
        },
        {
            "name": "Python Debugger: Huey",
            "type": "debugpy",
            "request": "launch",
            "args": [
                "run_huey"
            ],
            "django": true,
            "autoStartBrowser": false,
            "program": "${workspaceFolder}\\src\\manage.py",
            "env": {
                "FFMPEG_PATH": "ffmpeg",
                "FFPROBE_PATH": "ffprobe",
                "DEFAULT_DB_PWD": "755080f72c164f56b0038b2ebaacb8c2"
            }
        },
        {
            "name": "Vite",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceFolder}\\src",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run",
                "dev"
            ],
            "console": "integratedTerminal"
        }
    ]
}